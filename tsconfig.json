{
  "compilerOptions": {
    "paths": {
      "nitropack": [
        "./node_modules/nitropack"
      ],
      "defu": [
        "./node_modules/defu"
      ],
      "h3": [
        "./node_modules/h3"
      ],
      "consola": [
        "./node_modules/consola"
      ],
      "ofetch": [
        "./node_modules/ofetch"
      ],
      "@": [
        "."
      ],
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/stores/*": [
        "./src/stores/*"
      ],
      "assets": [
        "../assets"
      ],
      "assets/*": [
        "../assets/*"
      ],
      "public": [
        "../public"
      ],
      "public/*": [
        "../public/*"
      ],
    },
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "isolatedModules": true,
    "useDefineForClassFields": true,
    "strict": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "types": [
			"unplugin-vue-router/client"

		],
    "verbatimModuleSyntax": true,
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
		"./typed-router.d.ts",
    "../lib/**/*",
    "../components/**/*",
    "../composables/**/*",
    "../pages/**/*",
    "../store/**/*",
    "./"
  ],
  "exclude": [
    "./node_modules",
    "./dist",
    "./.output"
  ]
}
